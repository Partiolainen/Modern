{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseConfiguration": {
    "ConnectionString": "DataSource=local.db"
  },
  "Redis": {
    "ConnectionString": "127.0.0.1:6379"
  },
  "ModernCachedServiceConfiguration": {
    "AddToCacheWhenEntityCreated": true,
    "AddOrUpdateInCacheWhenEntityIsUpdated": true
  }
}
