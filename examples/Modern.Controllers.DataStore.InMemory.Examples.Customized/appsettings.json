{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseConfiguration": {
    "ConnectionString": "DataSource=local.db"
  },
  "ModernInMemoryServiceConfiguration": {
    "AddToCacheWhenEntityCreated": true,
    "AddOrUpdateInCacheWhenEntityIsUpdated": true
  }
}
